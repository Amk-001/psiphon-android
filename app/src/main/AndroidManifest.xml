<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.psiphon3.subscription"
    xmlns:tools="http://schemas.android.com/tools"
    android:installLocation="auto">
    <uses-sdk tools:overrideLibrary="ca.psiphon,
androidx.media2.common,
androidx.media2.player,
androidx.media2.session,
androidx.media2.widget,
com.adsbynimbus.android.nimbus,
com.adsbynimbus.core,
com.adsbynimbus.facebook,
com.adsbynimbus.openrtb,
com.adsbynimbus.render,
com.adsbynimbus.render.video,
com.adsbynimbus.render.web,
com.adsbynimbus.request,
com.adsbynimbus.request.okhttp,
com.adsbynimbus.ui,
com.amazon.device.ads,
com.criteo.publisher,
com.freestar.android.ads,
com.freestar.android.ads.admob,
com.freestar.android.ads.amazon,
com.freestar.android.ads.criteo,
com.freestar.android.ads.facebook,
com.freestar.android.ads.google,
com.freestar.android.ads.nimbus,
com.freestar.android.ads.pangle,
com.freestar.android.ads.tam,
com.freestar.android.ads.unityads,
com.freestar.android.ads.vungle,
com.freestar.android.ads.hyprmx,
com.freestar.android.ads.yahoo,
com.google.android.ads.consent,
com.google.android.exoplayer2,
com.google.android.exoplayer2.core,
com.google.android.exoplayer2.source.dash,
com.google.android.exoplayer2.source.hls,
com.google.android.exoplayer2.source.smoothstreaming,
com.google.android.exoplayer2.ui,
com.google.android.gms.ads,
com.google.android.gms.ads.impl,
com.google.android.gms.ads_base,
com.google.android.gms.gass,
com.google.android.gms.measurement.sdk.api,
com.google.android.gms.measurement_base,
com.google.ads.mediation.facebook,
com.hyprmx.android,
com.verizon.ads,
com.verizon.ads.edition,
com.verizon.ads.inlinewebadapter,
com.verizon.ads.inlineplacement,
com.verizon.ads.interstitialvastadapter,
com.verizon.ads.interstitialwebadapter,
com.verizon.ads.interstitialplacement,
com.verizon.ads.nativeverizonnativeadapter,
com.verizon.ads.nativeplacement,
com.verizon.ads.verizonnativecontroller,
com.verizon.ads.vastcontroller,
com.verizon.ads.videoplayer,
com.verizon.ads.webcontroller,
com.verizon.ads.webview,
com.verizon.ads.uriexperience,
com.verizon.ads.support,
com.verizon.ads.omsdk,
org.prebid.mobile.core,
com.freestar.android.ads.prebid,
com.freestar.android.ads.ogury" />
    <supports-screens
                  android:smallScreens="true"
                  android:normalScreens="true"
                  android:largeScreens="true"
                  android:xlargeScreens="true" />

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <!-- required for targetSdk 30+ to show all apps in the VPN exclusions dialog -->
    <uses-permission android:name="android.permission.QUERY_ALL_PACKAGES" />
    <!-- instruct manifest merger to remove any location permissions from the final manifest -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" tools:node="remove"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" tools:node="remove"/>
    <!-- instruct manifest merger to remove any external storage permissions from the final manifest -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" tools:node="remove"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" tools:node="remove"/>
    <uses-permission android:name="android.permission.VIBRATE"/>
    <uses-permission android:name="com.android.vending.BILLING"/>
    <uses-permission android:name="android.permission.FOREGROUND_SERVICE"/>

    <application
        android:name="com.psiphon3.PsiphonApplication"
        android:theme="@style/Theme.AppCompat"
        android:hardwareAccelerated="true"
        android:allowBackup="false"
        android:fullBackupContent="false"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:label="@string/app_name_psiphon_pro"
        android:usesCleartextTraffic="true"
        tools:ignore="GoogleAppIndexingWarning"
        tools:replace="android:label,android:fullBackupContent">

        <meta-data android:name="com.google.android.gms.version" android:value="@integer/google_play_services_version"/>

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="${admobAppId}"/>

        <activity
            android:name="com.psiphon3.MainActivity"
            android:label="@string/app_name_psiphon_pro"
            android:exported="true"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <!-- Accepts URIs in the form of "psiphon://settings” -->
                <data
                    android:scheme="psiphon"
                    android:host="settings" />
                <!-- Accepts URIs in the form of "psiphon://psicash” -->
                <data
                    android:scheme="psiphon"
                    android:host="psicash" />
                <!-- Accepts URIs in the form of "psiphon://subscribe” -->
                <data
                    android:scheme="psiphon"
                    android:host="subscribe" />
            </intent-filter>
        </activity>
        <activity-alias
            android:name="com.psiphon3.psiphonlibrary.TunnelIntentsHandler"
            android:exported="false"
            android:targetActivity="com.psiphon3.MainActivity" />
        <activity
            android:name="com.psiphon3.FeedbackActivity"
            android:parentActivityName="com.psiphon3.MainActivity"
            android:exported="true"
            android:label="@string/app_name_psiphon_pro">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.psiphon3.MainActivity" />
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <!-- Accepts URIs that begin with "psiphon://feedback” -->
                <data
                    android:scheme="psiphon"
                    android:host="feedback" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.psiphon3.psiphonlibrary.MoreOptionsPreferenceActivity"
            android:parentActivityName="com.psiphon3.MainActivity"
            android:label="@string/app_name_psiphon_pro">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.psiphon3.MainActivity" />
        </activity>
        <activity
            android:name="com.psiphon3.psicash.store.PsiCashStoreActivity"
            android:parentActivityName="com.psiphon3.MainActivity"
            android:label="@string/psicash_store_title">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.psiphon3.MainActivity" />
        </activity>
        <activity
            android:name="com.psiphon3.psicash.account.PsiCashAccountActivity"
            android:parentActivityName="com.psiphon3.MainActivity"
            android:label="@string/psicash_account_generic_title">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.psiphon3.MainActivity" />
        </activity>
        <activity
            android:name="com.psiphon3.psiphonlibrary.VpnOptionsPreferenceActivity"
            android:parentActivityName="com.psiphon3.MainActivity"
            android:label="@string/app_name_psiphon_pro">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.psiphon3.MainActivity" />
        </activity>
        <activity
            android:name="com.psiphon3.psiphonlibrary.ProxyOptionsPreferenceActivity"
            android:parentActivityName="com.psiphon3.MainActivity"
            android:label="@string/app_name_psiphon_pro">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.psiphon3.MainActivity" />
        </activity>
        <service
            android:name="com.psiphon3.PsiphonCrashService"
            android:exported="false"
            android:process=":PsiphonCrashService" />
        <service
            android:name="com.psiphon3.psiphonlibrary.TunnelVpnService"
            android:label="@string/app_name_psiphon_pro"
            android:permission="android.permission.BIND_VPN_SERVICE"
            android:exported="false"
            android:process=":TunnelVpnService" >
            <intent-filter>
                <action android:name="android.net.VpnService" />
            </intent-filter>
            <meta-data
                android:name="android.net.VpnService.SUPPORTS_ALWAYS_ON"
                android:value="true" />
        </service>
        <provider
        android:name="com.psiphon3.log.LoggingContentProvider"
        android:exported="false"
        android:process=":LoggingContentProvider"
        android:authorities="com.psiphon3.subscription.LoggingContentProvider" />
        <activity
            android:name="com.psiphon3.PaymentChooserActivity"
            android:parentActivityName="com.psiphon3.MainActivity"
            android:label="@string/app_name_psiphon_pro">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.psiphon3.MainActivity" />
        </activity>

    </application>

</manifest>
