    <string name="custom_proxy_headers_checkbox_description">سرتیتر های HTTP سفارشی را به همه درخواست‌های پروکسی HTTP خروجی اضافه کنید</string>
    <string name="custom_proxy_header_group_1">سرتیتر 1</string>
    <string name="custom_proxy_header_group_2">سرتیتر 2</string>
