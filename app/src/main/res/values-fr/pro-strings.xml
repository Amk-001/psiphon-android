    <!--  Summary text for 'Add custom HTTP headers' checkbox, appears underneath the checkbox title -->
    <string name="custom_proxy_headers_checkbox_description">Ajouter des en-têtes HTTP personnalisés à toutes les requêtes sortantes vers le mandataire</string>
    <string name="custom_proxy_header_group_1">En-tête 1</string>
    <string name="custom_proxy_header_group_2">En-tête 2</string>
    <string name="custom_proxy_header_group_3">En-tête 3</string>
    <string name="custom_proxy_header_group_4">En-tête 4</string>
    <string name="custom_proxy_header_group_5">En-tête 5</string>
    <string name="custom_proxy_header_group_6">En-tête 6</string>
    <string name="custom_proxy_header_name">Saisir le nom de l’en-tête</string>
    <string name="custom_proxy_header_value">Saisir la valeur de l’en-tête</string>
